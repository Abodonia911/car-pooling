generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ride {
  id           String   @id @default(uuid())
  driverId     String
  origin       String
  destination  String
  date         DateTime
  availableSeats Int
  createdAt    DateTime @default(now())
}
